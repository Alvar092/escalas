    },
    "scalesInfo.motricityIndex.recommendations" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Use as part of the clinical assessment to plan motor strength-focused rehabilitation interventions and to monitor recovery progress."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Usar como parte de la evaluación clínica para planificar intervenciones de rehabilitación centradas en fuerza motora y para seguimiento de la recuperación."
          }
        }
      }
    },
    "scalesInfo.motricityIndex.scoring" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Six items (3 for the upper limb: pinch grip, elbow flexion, shoulder abduction; and 3 for the lower limb: ankle dorsiflexion, knee extension and hip flexion).\nEach component is assessed with weighted scores and the total sum ranges from 0 to 100."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Seis ítems (3 para extremidad superior: prensión, flexión de codo, abducción de hombro; y 3 para extremidad inferior: dorsiflexión de tobillo, extensión de rodilla y flexión de cadera).\nCada componente se evalúa con puntuaciones ponderadas y la suma total varía de 0 a 100."
          }
        }
      }
    },
    "scalesInfo.trunkControlTest.description" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The Trunk Control Test assesses trunk control and stability through four functional movements designed to measure the patient's ability to initiate and perform trunk control without assistance."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "El Trunk Control Test evalúa el control y la estabilidad de tronco mediante cuatro movimientos funcionales destinados a medir la capacidad del paciente para iniciar y realizar control de tronco sin asistencia."
          }
        }
      }
    },
    "scalesInfo.trunkControlTest.interpretation" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "A higher total score indicates better trunk control. The TCT is used to assess postural function in patients with neurological disorders (e.g. stroke or spinal cord injury). Higher scores correlate with greater independence and functional capacity, including the likelihood of walking independently."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Una puntuación total más alta indica mejor control del tronco. El TCT se utiliza para evaluar la función postural en pacientes con alteraciones neurológicas (por ejemplo, ictus o lesión medular). Mejores puntuaciones se correlacionan con mayor independencia y capacidad funcional, incluyendo la probabilidad de caminar de manera independiente."
          }
        }
      }
    },
