    "scalesInfo.berg.recommendations" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "To detect changes, it is advisable to perform the test at least twice and, if possible, under the same conditions."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Para apreciar cambios, es aconsejable realizar la prueba al menos 2 veces y, a ser posible, bajo las mismas condiciones."
          }
        }
      }
    },
    "scalesInfo.berg.scoring" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Each item is scored from 0 to 4. A score of 0 indicates inability to perform the task and a score of 4 indicates full ability to perform the task.\nItems are scored based on time, level of independence or supervision required. If the patient requires supervision or assistance, this will be negatively reflected in the score."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Cada ítem puntúa de 0 a 4. Una puntuación de 0 indica incapacidad para realizar la tarea y una puntuación de 4 indica capacidad para poder hacer la tarea.\nLos ítems puntúan en relación al tiempo, nivel de independencia o supervisión requerida. Si el paciente necesita supervisión o asistencia, esto se verá reflejado en la puntuación negativamente."
          }
        }
      }
    },
    "scalesInfo.motricityIndex.description" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "The Motricity Index is an ordinal method for measuring motor strength in patients with hemiparesis following a stroke, based on selected movements of the upper and lower limbs to reflect the ability to generate force in key muscle groups."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "El Motricity Index es un método ordinal para medir la fuerza motora en pacientes con hemiparesia tras un accidente cerebrovascular, basado en movimientos seleccionados de extremidades superiores e inferiores para reflejar la capacidad de generar fuerza en grupos musculares clave."
          }
        }
      }
    },
    "scalesInfo.motricityIndex.interpretation" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "A higher score indicates greater overall motor strength. The Motricity Index is used to assess motor function in post-stroke patients and to monitor strength recovery."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Una puntuación más alta indica mayor fuerza motora global. El Motricity Index se utiliza para evaluar la función motora en pacientes post-ictus y monitorizar la recuperación de fuerza."
          }
        }
      }
    },
    "scalesInfo.motricityIndex.material.0" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
