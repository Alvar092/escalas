    "scalesInfo.berg.description" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "A widely used scale to assess static and dynamic balance in both sitting and standing positions."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Es una escala ampliamente usada para valorar el equilibrio estático y dinámico tanto en sedestación como en bipedestación."
          }
        }
      }
    },
    "scalesInfo.berg.interpretation" : {
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "0 to 20: Wheelchair dependent.\n21 to 40: Walks with assistance.\n41 to 56: Independent."
          }
        },
        "es" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "De 0 a 20: Precisa de silla de ruedas.\nDe 21 a 40: Puede caminar con ayuda.\nDe 41 a 56: Independiente."
          }
        }
      }
    },
